#include<iostream>
using namespace std;
int main()
{
    int n,r,c;
    cout<<"enter no. of rows::";
    cin>>r;
    cout<<"enter no. of coloums::";
    cin>>c;
    for(int i=1;i<=r;i++)
        {
          for(int j=1;j<=r+1-i;j++)
            {
                cout<<"* ";
            }  
            cout<<endl;
        }
}
