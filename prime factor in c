#include <stdio.h>
int main() 
{
    int i,j,k,num,factor;
    printf("enter the num\n");
    scanf("%d",&num);
    for(i=2;i<num;i++)
    {
          factor=1;
        if(num%i==0)
        {
            factor=0;
        }
        if(factor==0)
        {
            printf("the factors=%d\n",i);
        }
    }
    return 0;
}
