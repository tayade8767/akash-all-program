#include<iostream>
#include<stack>
#include<queue>
using namespace std;
int main()
{
    stack<int> s;
    stack<int> s1;
    s.push(10);
    s.push(11);
    s.push(12);
    s.push(13);
    s.push(14);
    s.push(15);
    cout<<s.top()<<endl;                           //   gives the 
    s.swap(s1);                                    //  swap the element from 1 to another stack
    cout<<s1.top()<<endl;
    while(!s.empty())
    {
        s.pop();                           //pop the element
    }
    s.push(10);
    cout<<"Size="<<s1.size()<<endl;;         //gives the size of stack
    cout<<s.top()<<endl;                           //   gives the top element in stack



queue<int> q;
    q.push(210);
    q.push(211);
    q.push(212);
    q.push(213);
    q.push(214);
    q.push(215);

cout<<"back="<<q.back()<<endl;               //  back function gives  last element from stack
cout<<"front="<<q.front()<<endl;               //  back function gives  first element from queae
q.emplace(00);                           // emplace insert the element from last  in stack or queae
cout<<q.back()<<endl;
while(!s.empty())
    {
        s.pop();                           //pop the element
    }

}
