#include<iostream>
using namespace std;
int main()
{
    int n,count=0,i;
    cout<<"enter the number"<<endl;
    cin>>n;
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        {
            count++;
        }
    }
    if(count==2)
    {
        cout<<"it prime number  "<<n;
    }
    else
    {
        cout<<"it is not prime number  "<<n;
    }
}
