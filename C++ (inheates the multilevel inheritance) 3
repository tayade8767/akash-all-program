#include<iostream>
using namespace std;
class A
{
  public:                                     //base/parent/super class
  A()
  {
      cout<<"base class 1"<<endl;
  }
};
class B:public A
{
  public:
  B()
  {
      cout<<"base class 2"<<endl;
  }
};
class c:public B                //derived/child/sub class
{

};
int main()
{
    c a;
}
