using namespace std;
 int main()
 {
	char name[25],akash[25];
	
	cout<<"enter the string 2"<<endl;
	cin.get(name, 25);           //  not read spaces
    cout << name<<endl;
	
//copy of string n represent the number of character
     cout<<strncpy(akash,name,2)<<endl;
     //copy of string
    cout<<"combination OR concatinate of string="<<strcpy(akash,name)<<endl;


