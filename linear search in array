  #include<stdio.h>
  int main()
  {
    int a[10],n,i,key;
    printf("enter the numbefr of data::");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter the data you want to search\n");
    scanf("%d",&key);
    for(i=0;i<n;i++)
    {
        if(a[i]==key)
        {
            printf("found\n");
        }
    }
  }
