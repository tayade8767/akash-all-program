int min(int a[5]);
int main() 
{
  int a[]={14,55,89,56,32},b[]={22,33,11,66},i;
   mini :
    cout<<"minimum no "<<a[0];
  cout<<"enter the first array"<<endl;
  for(i=0;i<5;i++)
    {
        cout<<"enter the"<<i<<"element"<<endl;
        cin>>a[i];
    }
   
    for(i=0;i<5;i++)
    {
         cout<<"enter the"<<i<<"element"<<endl;
        cin>>b[i];
    }
        min(a);
    min(b);
    return 0;
}

int min(int a[5])
{   
int p=a[0];
for(int i=0;i<=5;i++)
{
    if(a[0]>a[i])
    {
           a[0]=a[i];
    }
    else
    {
         p=a[i];
    }
}
cout<<"minimum"<<a[0];
}
