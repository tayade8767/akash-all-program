#include <iostream>
using namespace std;

int main() {
	int t,a,b,y;
	cin>>t;
	while(t--)
	{
	    cin>>a>>b;
	    int x=a>b?a:b;
	    for(int i=1;i<=a && i<=b;i++)
	    {
	        if(a%i==0&&b%i==0)
	        {
	            y=i;
	        }
	    }
	    cout<<y<<" ";
	    cout<<(a*b)/y<<endl;
	}
	return 0;
}
