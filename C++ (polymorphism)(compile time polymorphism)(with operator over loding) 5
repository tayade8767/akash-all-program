#include<iostream>
using namespace std;
class akash
{
  private:
  int num;
  public:
  akash()         //another way to represent the constructor
  {              // akash():num(8){}
      num=8;
  }
  void operator ++()
  {
      num=num+2;
  }
  void print()
  {
      cout<<"The Count is::"<<num;
  }
};
int main()
{
   akash a;
    ++a;
    a.print();
}
